    {
      "datasource": "Prometheus",
      "fieldConfig": {
        "defaults": {
          "thresholds": {
            "mode": "absolute",
            "steps": [
              { "color": "green", "value": null },
              { "color": "red", "value": 0.5 }
            ]
          },
          "mappings": [
            { "options": { "0": { "text": "DOWN" }, "1": { "text": "UP" } }, "type": "value" }
